{
  "name": "@forthright/vile",
  "version": "0.11.8",
  "description": "A punishing, yet easy to use tool for writing insightful code.",
  "main": "lib/index.js",
  "types": "src/@types/index.d.ts",
  "files": [
    "bin/vile",
    "lib",
    "src/@types",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">= 0.12",
    "npm": ">= 2.0.0"
  },
  "bin": {
    "vile": "bin/vile"
  },
  "scripts": {
    "babel": "babel",
    "coffee": "coffee",
    "compile": "node bin/compile",
    "compile-dev": "node bin/compile-dev",
    "clean": "rm -rf {.build,.test,.nyc_output,coverage,lib}",
    "dev": "sh bin/dev",
    "dev-setup": "sh bin/dev-setup",
    "gen-docs": "sh bin/gen-docs",
    "istanbul": "istanbul",
    "lint-cov": "node bin/lint-cov",
    "mocha": "mocha",
    "ncu": "ncu -a",
    "nyc": "nyc",
    "release-it": "release-it",
    "stats": "sloc -d -f cli-table -k total,source,comment -e 'fixtures|@types' src test",
    "test": "node bin/test-all",
    "test-non-system": "_mocha --recursive -u bdd -R spec -t 2000 .test/unit .test/integration",
    "test-non-system-cov": "nyc -r json -x .test -x **/node_modules/** --report-dir coverage/non-system npm run -s test-non-system",
    "test-system": "_mocha --recursive -u bdd -R spec -s 10000 -t 40000 .test/system",
    "test-system-cov": "cross-env TEST_COV=1 npm run -s test-system",
    "test-cov": "node bin/test-cov",
    "test-ci-build": "node bin/test-ci-build",
    "tsc": "tsc",
    "typedoc": "typedoc",
    "vile": "vile p -usi -x src.ts:lib.js,test.coffee:.test.js -n"
  },
  "author": "Brent Lintner <brent.lintner@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/forthright/vile/issues"
  },
  "homepage": "https://github.com/forthright/vile",
  "repository": {
    "type": "git",
    "url": "https://github.com/forthright/vile.git"
  },
  "keywords": [
    "quality",
    "analyze",
    "analyse",
    "lint",
    "check",
    "code",
    "tool"
  ],
  "dependencies": {
    "bluebird": "^3.4.7",
    "cli-spinner": "0.2.6",
    "commander": "^2.9.0",
    "extend": "^3.0.0",
    "git-diff-tree": "^1.0.0",
    "ignore-file": "^1.1.2",
    "inquirer": "^3.0.1",
    "js-yaml": "^3.8.1",
    "language-detect": "^1.1.0",
    "linez": "^4.1.1",
    "lodash": "^4.17.x",
    "minilog": "^3.1.0",
    "npm-run-path": "^2.0.2",
    "request": "^2.79.0",
    "unixify": "^0.2.1"
  },
  "peerDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/commander": "^2.3.31",
    "@types/extend": "^2.0.30",
    "@types/inquirer": "0.0.32",
    "@types/js-yaml": "^3.5.29",
    "@types/lodash": "^4.14.52",
    "@types/minilog": "^2.0.29",
    "@types/node": "^7.0.5",
    "@types/request": "0.0.39"
  },
  "devDependencies": {
    "@forthright/vile": ">=0.11.8",
    "@forthright/vile-coffeelint": ">=0.1.14",
    "@forthright/vile-comment": ">=0.1.17",
    "@forthright/vile-coverage": ">=0.2.14",
    "@forthright/vile-depcheck": ">=0.1.12",
    "@forthright/vile-eclint": ">=0.1.10",
    "@forthright/vile-escomplex": "^0.1.2",
    "@forthright/vile-eslint": ">=0.1.13",
    "@forthright/vile-git": ">=0.5.1",
    "@forthright/vile-language": ">=0.1.12",
    "@forthright/vile-license": ">=0.3.14",
    "@forthright/vile-ncu": ">=0.2.1",
    "@forthright/vile-newline": ">=0.1.12",
    "@forthright/vile-nsp": ">=0.2.13",
    "@forthright/vile-retire": "^0.1.2",
    "@forthright/vile-stat": "0.0.14",
    "@forthright/vile-tslint": ">=0.2.19",
    "@forthright/vile-whitespace": ">=0.1.11",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-script": "^1.12.3",
    "cross-env": "^3.1.4",
    "gh-pages": "^0.12.0",
    "istanbul": "^0.4.5",
    "mimus": "^0.2.3",
    "mocha": "^3.2.0",
    "npm-check-updates": "^2.10.2",
    "nyc": "^10.1.2",
    "pryjs": "^1.0.3",
    "release-it": "^2.5.4",
    "retire": "^1.2.11",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.6",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "sloc": "^0.2.0",
    "tslint": "^4.4.2",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.6",
    "wrench": "^1.5.9"
  }
}
